<script type="text/javascript" src="http://vkontakte.ru/js/api/openapi.js"></script>
<script type="text/javascript">
BX.ready(function(){VK.init({apiId: '2283932'});});

function BxVKAuthInfo(response) 
{
	var url_err = '/software/index.php?auth_service_id=VKontakte&auth_service_error=1';
	if(response.session) 
	{
		var url_post = '/software/index.php?login=yes';
		var url_ok = '/software/index.php';
		var data = {
			"auth_service_id": "VKontakte",
			"vk_session": response.session
		};
		BX.ajax.post(url_post, data, function(res){window.location = (res == "OK"? url_ok : url_err);});
	} 
	else 
	{
		window.location = url_err;
	}
}
</script>

