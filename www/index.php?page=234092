	$(document).ready(function(){              // по окончанию загрузки страницы
//			$(".activ_item").show();
		$("#MAIN_MENU ul li").mouseover(function() {
			$("li.select").removeClass("active");
			$(".SUB_MENU").hide();
			$(this).addClass("active");
			$(this).find(".SUB_MENU").show();
		}).mouseout(function(){
			$(this).removeClass("active").find(".SUB_MENU").hide();
			$("li.select").addClass("active");
			$("li.select.SUB_MENU").show();
		});
	});
</script>
